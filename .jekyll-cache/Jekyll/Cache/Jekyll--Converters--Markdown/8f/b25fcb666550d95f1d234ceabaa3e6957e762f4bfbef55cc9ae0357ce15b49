<script>
$(document).ready(function(){
  $('.icon_rotation').on({
    'click': function () {
      var origsrc = $(this).attr('src');
      var src = '';
      if (origsrc == 'http://localhost:4000/assets/0-expand_off.png') src = 'http://localhost:4000/assets/0-expand_on.png';
      if (origsrc == 'http://localhost:4000/assets/0-expand_on.png') src = 'http://localhost:4000/assets/0-expand_off.png';
      $(this).attr('src', src);
    }
  });
});
</script>

